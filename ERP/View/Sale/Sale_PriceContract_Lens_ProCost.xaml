                    </Grid>
                    <Grid Grid.Row="8">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="98"/>
                            <ColumnDefinition Width="105"/>
                            <ColumnDefinition Width="95"/>
                            <ColumnDefinition Width="105"/>
                            <ColumnDefinition Width="95"/>
                            <ColumnDefinition Width="105"/>
                            <ColumnDefinition Width="95"/>
                            <ColumnDefinition Width="105"/>
                            <ColumnDefinition Width="75"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>
                        <TextBlock Text="{Binding UIText[ERP_P1]}" Grid.Column="0"/>
