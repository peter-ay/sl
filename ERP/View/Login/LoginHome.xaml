                        </StackPanel>
                        <StackPanel Margin="5" HorizontalAlignment="Left">
                            <ex:ButtonHome_Transferring Margin="0,0,4,0"/>
                            <ex:ButtonHome_Inventory Margin="0,4,4,0"/>
                            <ex:ButtonHome_Transferring_Frame Margin="0,4,4,0"/>
