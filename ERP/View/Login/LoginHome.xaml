                            <ex:ButtonHome_BarCodeOut Margin="0,4,4,0"/>
                            <ex:ButtonHome_List_BarCode Margin="0,4,4,0"/>

                        </StackPanel>
                        <StackPanel Margin="5" HorizontalAlignment="Left">
                            <ex:ButtonHome_OtherIn  Margin="0,0,4,0"/>
