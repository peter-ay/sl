                        </StackPanel>
                        <StackPanel Margin="5" HorizontalAlignment="Left">
                            <ex:ButtonHome_Stocks Margin="0,0,4,0"/>
                            <ex:ButtonHome_Day Margin="0,4,4,0"/>
                            <ex:ButtonHome_Ledger Margin="0,4,4,0"/>
                        </StackPanel>
                    </StackPanel>

                    <StackPanel Orientation="Horizontal" >
                        <StackPanel Margin="5" HorizontalAlignment="Left">
