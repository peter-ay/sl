                            <ex:ButtonHome_List_InOut Margin="0,4,4,0"/>
                            <ex:ButtonHome_List_InOut_Frame Margin="0,4,4,0"/>
                        </StackPanel>
                        <StackPanel Margin="5" HorizontalAlignment="Left">
                            <ex:ButtonHome_List_Transferring Margin="0,4,4,0"/>
